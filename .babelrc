{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage" // 会自动按需引入polyfill
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-import",
      {
        "libraryName": "antd",
        "style": true //为true，使用antd组件的less样式文件，为css使用antd组件的css文件
      }
    ],
    // 开启对class公共字段的支持
    "@babel/plugin-proposal-class-properties"
  ]
}
